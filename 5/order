#!/usr/bin/env python3

def main():
	    # Return elements generated by a number a in Zp
	def generator(a, zp):
	    elements = [1]
	    count = 1
	    while((a ** count) % zp != 1):
	        elements.append((a ** count) % zp)
	        count += 1
	    return elements
	
	# Prime number test
	# returns True if the parameter is a prime number
	def isprime(a):
	    return not (a < 2 or any(a % x == 0 for x in range(2, int(a ** 0.5) + 1)))
	
	# Get/process user input
	print("Enter a and prime (space separated): ", end="")
	i = list(map(int, input().split()))
	# Confirm the user entered a prime
	if not isprime(i[1]):
	    raise Exception("Second value is not prime")
	
	# Run the generator on the user input and print the result
	gen = generator(i[0], i[1])
	print("Generator of {0} in Z{1}: ".format(i[0], i[1]), gen)
	print("Order of {0} in Z{1}: ".format(i[0], i[1]), len(gen))
	
if __name__ == '__main__':
    main()
